Dictionary({
  Name: { domain: 'Nomen', required: true },
  Act: Action({
    getMethod: (session, { Id }, callback) =>
      callback(null, Id === 42 ? 'M1' : 'M2'),
    Methods: {
      M1: {
        Execute: (session, { Id }, callback) =>
          callback(null, `M1Resource${Id}`),
        // `Form` will be resolved as 'CustomActionsMethods.M1'
      },
      M2: {
        Execute: (session, { Id }, callback) =>
          callback(null, `M2Resource${Id}`),
        Form: 'CustomForm',
      }
    },
  }),
})
